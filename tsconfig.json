{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "dist",
    "lib": ["esnext", "dom", "dom.iterable", "webworker"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "checkJs": true
    // "noEmit": true
  },
  "exclude": ["_build", "dist", "backend/wasm/shims", "vendor"]
}
