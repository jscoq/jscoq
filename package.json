{
  "name": "jscoq",
  "version": "0.16.1",
  "type": "module",
  "description": "A port of Coq to JavaScript -- run Coq in your browser",
  "main": "frontend/classic/js/index.js",
  "browser": {
    "path": "path-browserify",
    "stream": "stream-browserify",
    "tty": false,
    "url": false,
    "worker_threads": false,
    "fs": false,
    "crypto": false
  },
  "bin": {
    "jscoq": "./dist-cli/cli.cjs",
    "jscoqdoc": "frontend/classic/js/jscoqdoc.cjs"
  },
  "dependencies": {
    "@codemirror/state": "^6.1.1",
    "@codemirror/view": "^6.2.4",
    "@types/codemirror": "^5.60.5",
    "array-equal": "^1.0.0",
    "browser-or-node": "^2.0.0",
    "child-process-promise": "^2.2.1",
    "codemirror": "^5.61.0",
    "commander": "^5.0.0",
    "fflate-unzip": "^0.7.0",
    "find": "^0.3.0",
    "glob": "^7.1.3",
    "jquery": "^3.6.0",
    "jszip": "^3.5.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.20",
    "mkdirp": "^1.0.4",
    "neatjson": "^0.10.5",
    "path": "^0.12.7",
    "proper-lockfile": "^4.1.2",
    "prosemirror-example-setup": "^1.2.1",
    "prosemirror-highlightjs": "0.9.1",
    "prosemirror-markdown": "^1.9.3",
    "prosemirror-schema-basic": "^1.2.0",
    "prosemirror-state": "^1.4.1",
    "prosemirror-view": "^1.27.0",
    "throttle-debounce": "^5.0.0",
    "vue": "^2.6.12",
    "vue-context-menu": "^2.0.6",
    "wacoq-deps": "^0.1.0"
  },
  "devDependencies": {
    "@corwin.amber/hastebin": "^0.1.2",
    "@types/find": "^0.2.1",
    "@types/glob": "^8.0.1",
    "@types/jquery": "^3.5.14",
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^13.11.1",
    "@types/pako": "^2.0.0",
    "@types/throttle-debounce": "^5.0.0",
    "assert": "^2.0.0",
    "bootstrap": "^5.2.1",
    "buffer": "^4.9.2",
    "constants-browserify": "^1.0.0",
    "css-loader": "^5.2.4",
    "esbuild": "^0.17.4",
    "esbuild-wasm": "^0.17.4",
    "file-loader": "^6.0.0",
    "mocha": "^9.2.2",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "ronin-p2p": "^0.1.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "files": [
    "frontend",
    "backend/*.js",
    "backend/*.ts",
    "backend/jsoo/*.bc.cjs",
    "backend/wasm/*.bc",
    "backend/wasm/*.wasm",
    "coq-pkgs",
    "dist",
    "examples",
    "docs",
    "jscoq.js",
    "index.html"
  ],
  "scripts": {
    "esbuild": "node esbuild.mjs",
    "release": "node esbuild.mjs --minify --sourcemap=no",
    "typecheck": "tsc -b .",
    "watch": "node esbuild.mjs --watch",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jscoq/jscoq.git"
  },
  "author": "ejgallego",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/jscoq/jscoq/issues"
  },
  "homepage": "https://jscoq.github.io"
}
